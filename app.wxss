/**app.wxss**/
@import "weui.wxss";

.container {
  height: 100%;
  box-sizing: border-box;
}

.swiper {
  height: 300rpx;
  width: 100%;
  text-align: center;
}

.swiper image {
  height: 100%;
  width: 100%;
}

button::after {
  border: none;
}

.footer_title {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.footer {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  background: #f7f7fa;
  position: fixed;
  bottom: 0;
  z-index: 9999;
  height: 100rpx;
  width: 100%;
  padding-top: 10rpx;
}

.footerActive {
  color: #ffc824;
}

.footer image {
  width: 50rpx;
  height: 50rpx;
}

.footer_title {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #666;
}

.shou {
  display: flex;
  justify-content: center;
}

.shou image {
  border-radius: 50%;
  width: 120rpx;
  height: 120rpx;
  margin-top: 20rpx;
}

.shouquan {
  color: rgb(4, 156, 49);
}

.invisibleclass {
  display: flex;
  flex-direction: column;
  /* margin-top: 40rpx; */
  
}