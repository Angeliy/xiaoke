page {
  background: #f2f4f6;
}

.container {
  border: 1px solid #eee;
}

.palace, .top {
  display: flex;
}

.one {
  width: 50%;
  border-bottom: 1px solid #eee;
}

.two {
  width: 50%;
}

.three {
  width: 50%;
  border-right: 1px solid #eee;
}

navigator {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 50rpx 0;
}

image {
  margin: 0 auto;
  width: 120rpx;
  height: 120rpx;
  display: block;
}

text {
  font-size: 35rpx;
  font-weight: bold;
  color: #333;
  text-align: center;
}
