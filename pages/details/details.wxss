page {
  background: #f2f4f6;
}

.content {
  background: #fff;
  margin-bottom: 20rpx;
  padding: 30rpx;
}

.title {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.tt text{
  color: #ffc824;
}

.see_more{
  font-size: 30rpx;
}

.title text {
  display: block;
}

.title text {
  font-size: 35rpx;
  color: #333;
  font-weight: bold;
}

.share text {
  font-size: 28rpx;
  color: #666;
}

.stylist_photo image {
  width: 140rpx;
  height: 140rpx;
  vertical-align: middle;
}

.stylist_text {
  display: flex;
  flex-direction: column;
  color: #666;
  flex: 1;
  margin-left: 30rpx;
}

.stylist {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 25rpx 0;
  font-size: 30rpx;
}

.stylist_title text {
  margin-right: 30rpx;
}

.stylist_title .p {
  color: #333;
  font-weight: bold;
}

.star text {
  color: #333;
  padding-right: 10rpx;
}

.stylist_text image {
  height: 25rpx;
  width: 25rpx;
  display: inline-block;
  margin: 0 5rpx;
}

.score text {
  color: rgb(133, 133, 133);
  margin-right: 30rpx;
}

.good {
  margin-bottom: 25rpx;
}

.good text {
  display: block;
  font-size: 30rpx;
  color: #333;
}

.good text:nth-child(2) {
  padding-top: 10rpx;
  font-size: 26rpx;
  color: #666;
  display: block;
}

.content .text {
  text-align: center;
  color: #333;
  font-size: 30rpx;
  margin-bottom: 30rpx;
}

/* 多选按钮样式 */

.label-1 {
  border-bottom: 1px solid #eee;
  padding: 20rpx 0;
  display: flex;
  flex-direction: row;
}

.label-1__icon {
  position: relative;
  margin-right: 10px;
  display: inline-block;
  vertical-align: middle;
  width: 18px;
  height: 18px;
  background: #eee;
}

.label-1__icon-checked {
  position: absolute;
  top: 3px;
  left: 3px;
  width: 12px;
  height: 12px;
  background: #ffc824;
}

.service {
  font-size: 30rpx;
  color: #333;
  margin-top: 30rpx;
}

.check {
  color: #666;
}

.check text {
  margin: 0 10rpx;
}

.picker {
  font-size: 30rpx;
  color: #666;
  border: 1px solid #ccc;
  margin: 15rpx;
  padding: 0 25rpx;
  border-radius: 6rpx;
}

.section {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.works {
  width: 100%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.works image {
  width: 225rpx;
  height: 225rpx;
  margin: 5rpx 0;
}

.btn button {
  background: #ffc824;
  border: 1px solid #333;
  line-height: 65rpx;
  height: 70rpx;
  font-size: 30rpx;
  margin: 50rpx 30rpx;
  margin-bottom: 30rpx;
}
.clickview{
  width: 750rpx;
  height: 100rpx;
  line-height: 100rpx;
  z-index: 100;
}
.clickview text{
  z-index: -1;
}
/**
 * 授权提示
 */
.drawer_box {
width: 600rpx;
overflow: hidden;
position: fixed;
top: 50%;
left: 3%;
z-index: 1001;
background: #f8f8f8;
margin: -150px 50rpx 0 50rpx;
border-radius: 3px;
}
.drawer_title {
font-size: 30rpx;
color: #666;
padding: 15px;
border-bottom: 1px solid #e8e8ea;
text-align: center;
}

.customerPhone {
  font-size: 30rpx;
  color: #333;
  margin-top: 30rpx;
}