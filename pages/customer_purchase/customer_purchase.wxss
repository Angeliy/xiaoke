page {
  background: #f2f4f6;
}

.swiper-tab {
  width: 100%;
  height: 100%;
  border-bottom: 2rpx solid #fff;
  text-align: center;
  line-height: 80rpx;
  background: #fff;
}

.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  width: 25%;
  color: #333;
}

.on {
  color: #ffcc3c;
  border-bottom: 5rpx solid #ffcc3c;
}

.scoll-h {
  height: 100%;
  margin: 20rpx 0;
}

.line {
  display: flex;
  flex-direction: row;
  justify-content: center;
  padding: 20rpx 30rpx;
  background: #fff;
  border-bottom: 1px solid #eee;
  margin-bottom: 20rpx;
}

.gg button {
  width: 166rpx;
  height: 50rpx;
  line-height: 47rpx;
  font-size: 25rpx;
  background: #ffc824;
  color: #333;
  border: 1px solid #333;
  border-radius: 15rpx;
  margin-top: 20rpx;
  float: right;
}

.line image {
  width: 160rpx;
  height: 160rpx;
  margin-right: 50rpx;
}

.cc {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.p1 {
  width: 500rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 35rpx;
  color: #333;
}

.p2 {
  color: red;
}

.p3 {
  width: 65rpx;
  height: 40rpx;
  background: #ffc824;
  color: #fff;
  font-size: 25rpx;
  line-height: 40rpx;
  text-align: center;
}

.p4 {
  font-size: 26rpx;
  color: #666;
  margin-right: 77rpx;
}

.p6 {
  font-size: 25rpx;
  color: #666;
}

.p5 {
  font-size: 25rpx;
  color: #ffc824;
}

